{
  "mcpServers": {
    "task-master-ai": {
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],
      "env": {
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
        "MISTRAL_API_KEY": "${MISTRAL_API_KEY}",
        "GROQ_API_KEY": "${GROQ_API_KEY}",
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
        "XAI_API_KEY": "${XAI_API_KEY}",
        "AZURE_OPENAI_API_KEY": "${AZURE_OPENAI_API_KEY}",
        "OLLAMA_API_KEY": "${OLLAMA_API_KEY}"
      },
      "type": "stdio"
    },
    "leantime-mcp": {
      "command": "node",
      "args": ["src/integrations/leantime_mcp_server.js"],
      "env": {
        "LEANTIME_API_URL": "${LEANTIME_API_URL:-http://localhost:8080}",
        "LEANTIME_API_TOKEN": "${LEANTIME_API_TOKEN}",
        "DOPEMUX_INTEGRATION_KEY": "${DOPEMUX_INTEGRATION_KEY}"
      },
      "type": "stdio"
    },
    "mas-sequential-thinking": {
      "command": "uv",
      "args": ["--directory", "docker/mcp-servers/mcp-server-mas-sequential-thinking", "run", "mcp-server-mas-sequential-thinking"],
      "env": {
        "LLM_PROVIDER": "github",
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "GITHUB_TEAM_MODEL_ID": "openai/gpt-5",
        "GITHUB_AGENT_MODEL_ID": "openai/gpt-5-min",
        "EXA_API_KEY": "8f180834-2db5-4574-8b68-f8512628ee66",
        "ENABLE_CONTEXT_PRESERVATION": "true",
        "ENABLE_GENTLE_GUIDANCE": "true",
        "MAX_THINKING_STEPS": "10"
      },
      "type": "stdio"
    }
  }
}