{
  "mcpServers": {
    "conport": {
      "command": "/Users/hue/code/dopemux-mvp/services/conport/venv/bin/conport-mcp",
      "args": [
        "--mode", "stdio",
        "--workspace_id", "/Users/hue/code/dopemux-mvp"
      ],
      "env": {},
      "type": "stdio"
    },
    "metamcp-broker": {
      "command": "uvx",
      "args": [
        "mcp-proxy",
        "--transport",
        "streamablehttp",
        "http://127.0.0.1:8091"
      ],
      "env": {},
      "type": "stdio"
    },
    "gpt-researcher_wrapped": {
      "command": "python",
      "args": [
        "/Users/hue/code/dopemux-mvp/scripts/mcp_event_wrapper.py",
        "--server", "gpt-researcher",
        "--instance", "${DOPEMUX_INSTANCE_ID:-A}",
        "--redis-host", "localhost",
        "--redis-port", "6379"
      ],
      "env": {
        "WORKSPACE_PATH": "/Users/hue/code/dopemux-mvp",
        "EXA_API_KEY": "${EXA_API_KEY}",
        "TAVILY_API_KEY": "${TAVILY_API_KEY}",
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}",
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}",
        "DEBUG": "false",
        "DOPEMUX_INSTANCE_ID": "${DOPEMUX_INSTANCE_ID:-A}"
      },
      "type": "stdio"
    },
    "serena": {
      "command": "uvx",
      "args": [
        "mcp-proxy",
        "--transport",
        "streamablehttp",
        "http://127.0.0.1:3006/sse"
      ],
      "env": {},
      "type": "stdio"
    },
    "task-master-ai": {
      "command": "npx",
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "env": {},
      "type": "stdio"
    },
    "mas-sequential-thinking": {
      "command": "docker",
      "args": [
        "exec",
        "mcp-mas-sequential-thinking",
        "mcp-server-mas-sequential-thinking"
      ],
      "env": {},
      "type": "stdio"
    },
    "zen": {
      "command": "uvx",
      "args": [
        "--from", "git+https://github.com/BeehiveInnovations/zen-mcp-server.git",
        "zen-mcp-server"
      ],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}",
        "GROQ_API_KEY": "${GROQ_API_KEY}"
      },
      "type": "stdio"
    },
    "context7": {
      "command": "npx",
      "args": [
        "@upstash/context7-mcp"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "type": "stdio"
    },
    "gpt-researcher": {
      "command": "python",
      "args": [
        "/Users/hue/code/dopemux-mvp/scripts/gpt-researcher/mcp_server.py"
      ],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "TAVILY_API_KEY": "${TAVILY_API_KEY}",
        "VOYAGEAI_API_KEY": "${VOYAGEAI_API_KEY}",
        "EXA_API_KEY": "${EXA_API_KEY}"
      },
      "type": "stdio"
    }
  }
}